#include <stdio.h>
int
main ()
{
  int num, count,i;
  printf ("type the number \n");
  scanf ("%d",&num);
  for(i=1;i<=num;i++)
  {
  if ((num % i==0))
  count=count+1;
  }
  if(num==1)
  printf("Number is neither prime nor compoaite");
  else
  {
    if (count==2) 
        printf ("its a prime number" );
    else
     printf ("its not a prime number");
  }
  return 0;
}
